{% extends "SofApiBundle::layout.html.twig" %}
{% block content %}
    {#module Driver#}
    <div id="Driver_Load" action="{{ path('Driver_Load') }}"></div>
    <div id="Driver_Update" action="{{ path('Driver_Update') }}"></div>
    <div id="Driver_Delete" action="{{ path('Driver_Delete') }}"></div>

    {#module User#}
    <div id="User_Load" action="{{ path('User_Load') }}"></div>
    <div id="User_Update" action="{{ path('User_Update') }}"></div>
    <div id="User_Delete" action="{{ path('User_Delete') }}"></div>

    {#module Customer#}
    <div id="Customer_Load" action="{{ path('Customer_Load') }}"></div>
    <div id="Customer_Update" action="{{ path('Customer_Update') }}"></div>
    <div id="Customer_Delete" action="{{ path('Customer_Delete') }}"></div>

    {#module Product#}
    <div id="Product_Load" action="{{ path('Product_Load') }}"></div>
    <div id="Product_Update" action="{{ path('Product_Update') }}"></div>
    <div id="Product_Delete" action="{{ path('Product_Delete') }}"></div>

    {#Logout path#}
    <div id="user_logout" action="{{ path('user_logout') }}"></div>

{% endblock %}
